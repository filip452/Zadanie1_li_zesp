
CPPFLAGS= -c -g -Iinc -Wall -pedantic

__start__: szablony
	./szablony

szablony: obj/main.o obj/wekC.o 
	g++ -Wall -pedantic -o szablony obj/main.o obj/wekC.o

obj/main.o: src/main.cpp inc/wek.hh src/wek.cpp src/wekC.cpp
	g++ ${CPPFLAGS} -o obj/main.o src/main.cpp
#
#obj/LZ.o: inc/LZ.hh
#	g++ ${CPPFLAGS} -o obj/LZ.o 

obj/wekC.o: src/wek.cpp src/wekC.cpp inc/wek.hh
	g++ ${CPPFLAGS} -o obj/wekC.o src/wekC.cpp

clean:
	rm -f obj/*.o szablony
